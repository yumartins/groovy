{
  "name": "groovy-dash",
  "version": "0.0.1",
  "description": "Dashboard for app music",
  "main": "index.js",
  "author": "Yuri Martins",
  "private": false,
  "scripts": {
    "app": "node ./index.js",
    "build": "next build && next export",
    "develop": "next"
  },
  "babel": {
    "presets": [
      "next/babel",
      "module:groovy-babel"
    ],
    "plugins": [
      "@babel/plugin-proposal-do-expressions",
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": false,
          "preprocess": false,
          "minify": true
        }
      ]
    ]
  },
  "dependencies": {
    "@svgr/webpack": "^5.4.0",
    "@unform/web": "^2.1.3",
    "express": "^4.17.1",
    "next": "^11.1.3",
    "next-fonts": "^1.1.0",
    "next-images": "^1.4.0",
    "prop-types": "^15.7.2",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.8",
    "react-id-swiper": "^4.0.0",
    "swiper": "^6.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-do-expressions": "^7.10.1"
  }
}
